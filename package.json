    "@types/node": "~8.9.5",
    "@types/object-path": "^0.9.29",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^5.2.1",
    "cypress": "3.4.1",
    "dotenv": "6.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "0.4.0",
